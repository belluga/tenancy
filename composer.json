{
    "name": "belluga/tenancy",
    "description": "Automatic multi-tenancy for your Laravel application.",
    "keywords": ["laravel", "multi-tenancy", "multi-database", "tenancy"],
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Å tancl",
            "email": "samuel.stancl@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "facade/ignition-contracts": "^1.0.2",
        "ramsey/uuid": "^4.7.3",
        "stancl/jobpipeline": "^1.6.2",
        "stancl/virtualcolumn": "^1.3.1",
        "mongodb/laravel-mongodb": "4.5"
    },
    "require-dev": {
        "laravel/framework": "^9.0|^10.0|^11.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "league/flysystem-aws-s3-v3": "^3.12.2",
        "doctrine/dbal": "^3.6.0",
        "spatie/valuestore": "^1.3.2"
    },
    "autoload": {
        "psr-4": {
            "Belluga\\Tenancy\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Belluga\\Tenancy\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Belluga\\Tenancy\\TenancyServiceProvider"
            ],
            "aliases": {
                "Tenancy": "Belluga\\Tenancy\\Facades\\Tenancy",
                "GlobalCache": "Belluga\\Tenancy\\Facades\\GlobalCache"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
